{
  "manifest_version": 2,

  "name": "Zindagi",
  "description": "This extension will help patients navigate MyChart",
  "version": "1.0",
  "background": {
    "scripts": ["popup.js"],
    "persistent": false
  },

  "browser_action": {
   "default_icon": "zindagi.png"
  },
  "permissions": [
   "activeTab",
   "tabs", 
   "<all_urls>"
   ]
}
